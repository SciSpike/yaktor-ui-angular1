	<script>
	    var hash = CryptoJS.SHA3("password");
	    var hashString = hash.toString(CryptoJS.enc.Base64);
	    
	    /*var hash2 = CryptoJS.SHA256("Message");
	    console.log(hash.toString(CryptoJS.enc.Base64));*/
	
		function hex2a(hex) {
		    var str = '';
		    for (var i = 0; i < hex.length; i += 2)
		        str += String.fromCharCode(parseInt(hex.substr(i, 2), 16));
		    return str;
		}
	
	    var encrypted = CryptoJS.AES.encrypt("This is an encrypted message", hashString);
	    var decrypted = CryptoJS.AES.decrypt(encrypted, hashString);
	    
	    console.log('decrypted string');
	    console.log(hex2a(decrypted.toString()));
	</script>
  
